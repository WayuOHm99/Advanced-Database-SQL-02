SELECT HOUR_SEQ.NEXTVAL
FROM dual;

SELECT *
FROM USER_OBJECTS
WHERE OBJECT_TYPE = 'SEQUENCE'
 AND OBJECT_NAME = 'HOUR_SEQ';

SELECT *
FROM USER_CONSTRAINTS
WHERE CONSTRAINT_NAME LIKE 'NEMP%';

SELECT *
FROM EMP_DETAILS;

SELECT * FROM (
     SELECT ROWNUM AS rank, tm.*
     FROM TOP_MANAGERS tm
);

SELECT HOUR_SEQ.CURRVAL AS current_value
FROM DUAL;

SELECT HOUR_SEQ.NEXTVAL AS next_value
FROM DUAL;

SELECT HOUR_SEQ.CURRVAL AS current_value
FROM DUAL;
