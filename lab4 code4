INSERT INTO NEW_EMPLOYEES_V2 (
    EMPLOYEE_ID,
    FIRST_NAME,
    LAST_NAME,
    EMAIL,
    PHONE_NUMBER,
    HIRE_DATE,
    JOB_ID,
    SALARY,
    DEPARTMENT_ID
)
SELECT 
    EMPLOYEE_ID,
    FIRST_NAME,
    LAST_NAME,
    EMAIL,
    SUBSTR(PHONE_NUMBER, 1, 15),
    HIRE_DATE,
    JOB_ID,
    SALARY,
    DEPARTMENT_ID
FROM EMPLOYEES;
